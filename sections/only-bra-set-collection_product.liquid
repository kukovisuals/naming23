<section class="premium_product_section same__cols">
  <div class="section-wrap">
    {% for block in section.blocks %}
      {% assign showcaseCollection = collections['only-bra-showcase-collection'] %}
      {% assign collection = collections[block.settings.collection] %}
        <div class="collection_group" id="{% if block.settings.collection contains "port" %}port-assortment{% else %}parisiansummer-assortment{% endif %}">
        <div class="collection_group_head">
          <h4 class="text-sectiontlt23">The only bra you’ll ever need</h4>
          <p class="text-sectiontxt23">You’ve never felt anything like this before</p>
        </div>
        <!--technology section -->
        <section class="permium_hero_sectio eby-bnnr-lg">
          <div class="section-wrap eby-sm-margin1">
            <div class="section_image iphone_only">
              <img
                data-src="https://cdn.shopify.com/s/files/1/0313/4062/5964/files/LP---376---only-bra---usp-bourbon-updated.jpg"
                loading="lazy"
                class="mobile_image lazyload"
                width="376"
                height="752"
                alt="Only Bra Technology"
                title="Only Bra Technology" />
            </div>
            <div class="section_image ipads_only">
              <img
                data-src="https://cdn.shopify.com/s/files/1/0313/4062/5964/files/LP---1440---only-bra---usp-bourbon-updated.jpg"
                loading="lazy"
                class="ipad_image lazyload"
                width="762"
                height="762"
                alt="Only Bra Technology"
                title="Only Bra Technology" />
            </div>
            <div class="section_image laptop_only eby-max-wd-banner">
              <img
                data-src="https://cdn.shopify.com/s/files/1/0313/4062/5964/files/LP---1440---only-bra---usp-bourbon-updated.jpg"
                loading="lazy"
                class="desktop_image lazyload"
                width="1680"
                height="993"
                alt="Only Bra Technology"
                title="Only Bra Technology" />
            </div>
          </div>
        </section>

        <div class="premium_product_section eby-premium_content_block velaCollections" data-coll_handle="silk-lp-full-assortment" data-coll_title="Sheer LP, Full Assortment" id="product_listing_section">
          <div class="product-grd collection_group fullwidth">
            <div class="products_cols collection_group_list half_and_half-view">

              {% for product in showcaseCollection.products limit: 3 %}

                {%- assign itemNumberBeforeImage = forloop.index -%}

                  {% if itemNumberBeforeImage > 0 %}
                   {%- assign hasFlag = "false" -%}
                  {% assign isOnHolidaySale = false %}
                  {% assign on_sale = false %}
                  <!-- fc 041123 -->
                  {% assign isBundlePack = false %}
                  {% assign bndlVariants = '' %}
                  {% assign bndlVariants2 = '' %}
                  {% assign packProducts = '' %}
                  {% assign isBundleBraExtra = '' %}
                  {% assign packPrice = '' %}
                  
                  {% if product.type == "Bra Pack" %}
                    {% assign isBundleBraExtra = product.compare_at_price | money_without_trailing_zeros %}
                  {% endif %}
                  {% if product.type contains "Cotton" %}
                      {%- assign showQuickAdd = true -%}
                  {% elsif product.type contains "Bra Pack" or product.type contains "Season Set" %}
                      {%- assign showQuickAdd = false -%}
                  {% endif %}

                  {% for prodTag in product.tags %}
                    {% if prodTag contains "lds2023" or prodTag contains "eoy2022-" %}
                      {% assign isOnHolidaySale = true %}
                      {% assign on_sale = true %}
                    {%- elsif prodTag contains 'fyca23' %}
                        {% assign bndlVariants = prodTag | remove: 'fyca23-' %}
                        {% assign isBundlePack = true %}
                    {% elsif prodTag contains 'fycb23' %}
                        {% assign bndlVariants2 = prodTag | remove: 'fycb23-' %}
                    {%  elsif prodTag contains 'packProducts' %}
                        {% assign packProducts = prodTag | remove: 'packProducts-' %}   
                    {%  elsif prodTag contains 'packPrice' %}
                        {% assign packPrice = prodTag | remove: 'packPrice-' %}
                    {% endif %}
                    {% if prodTag contains "prodFlag-" %}
                        {% assign hasFlag = "true" %}
                        {% assign prodFlag = prodTag | replace: "prodFlag-", "" | replace: "_", " " | capitalize %}
                    {% endif %}
                  {% endfor %}
                  
                  {% if isOnHolidaySale %}
                    {% assign isSubscriber = false%}
                    
                    {% if customer.tags contains 'Active Subscriber' %}
                      {% assign isSubscriber = true%}
                    {% endif %}
                    
                    {% assign premSkuCodes = ""%}
                    {% assign holidayDiscounts = "20%,20%" | split: ',' %}
                    {% assign holidayCodes = "SK,BU" | split: "," %}
                    {% assign currSkuCode = product.selected_or_first_available_variant.sku | slice: 7, 2 %}
                    
                    {% if holidayCodes contains currSkuCode %}
                      {% for skuCode in holidayCodes %}
                        {% if currSkuCode == skuCode %}
                          {% assign skuCodeIndex = forloop.index0 %}
                          {% break %}
                        {% endif %}
                      {% endfor %}
                    {% endif %}
                  
                    {% assign currSaleDiscount = holidayDiscounts[skuCodeIndex] %}
                    {% elsif product.type == "Season Set" %}
                      {% assign currSaleDiscount = "30%" %}
                    {% elsif product.type == "Bra Pack" %}
                      {% assign currSaleDiscount = "30%" %}
                      {% for prodTag in product.tags %}
                        {% if prodTag contains 'bundleFabric-mesh_and_signature'%}
                          {% assign currSaleDiscount = "14.51%" %}
                        {% endif %}
                      {% endfor %}
                    {% if isSubscriber %}
                        {% assign currSaleDiscount = currSaleDiscount | replace: "%", "" | plus: 10 | append: "%" %}
                    {% endif %}
                    
                  {% endif %}

                                
                  {% if product.type == "Season Set" %}
                    {% for prodTag in product.tags %}
                      {% if tempFab contains 'setTempFa-' %}
                        {%- assign prodFabricTitle = "Only Bra & Panty Set" -%}
                      {% elsif prodTag contains "setFabric-" %}
                        {%- assign glueFab = ""-%}
                        {%- assign tempFab = prodTag | replace: "setType-", "" -%}
                        {% if tempFab contains 'mesh' %}
                          {%- assign glueFab = "Bralette & "-%}  
                          {%- assign prodFabricTitle = "Sheer Bra & Panty Set" -%}
                        {% elsif tempFab contains 'signature' %}
                          {%- assign glueFab = "Support & "-%}
                          {%- assign prodFabricTitle = "Bra & Panty Set" -%}
                        {% endif %}
                      {% elsif prodTag contains "setType-" %}
                        {%- assign gluePanty = ""-%}
                        {%- assign tempFab = prodTag | replace: "setType-", "" -%}
                        {% if tempFab contains 'high_waisted' %}
                          {%- assign gluePanty = "High Waisted"-%}  
                        {% elsif tempFab contains 'high_cut' %}
                          {%- assign gluePanty = "High Cut"-%}
                        {% elsif tempFab contains '_brief' %}
                          {%- assign gluePanty = "Brief"-%}  
                        {% elsif tempFab contains '_thong' %}
                          {%- assign gluePanty = "Thong"-%}  
                        {% endif %}
                      {% endif %}
                    {% endfor %}
                    {% assign isBundleSetFrabric =  glueFab | append: gluePanty %}
                  {% elsif product.type contains "Core Bra" %}
                    {%- assign prodNameTitle = "Seamless" -%}
                    {% for prodTag in product.tags %}
                        {% if prodTag contains "braletteFabric-" %}
                            {% if prodTag == "braletteFabric-signature" %}
                                {%- assign prodFabricTitle = "Only Bra" -%}
                            {% elsif prodTag == "braletteFabric-cotton" %}
                                {%- assign prodFabricTitle = "Err add p type" -%}
                                    {% else %}
                                        {% if product.type contains "Mesh" %}
                                    {%- assign prodFabricTitle = "Sheer Bralette" -%}
                                {% else %}
                                    {%- assign prodFabricTitle = "Err Add p type" -%}
                                {% endif %}
                            {% endif %}
                        {% endif %}
                    {% endfor %}
                  {% endif %}
                  {%- assign showQuickAdd = false -%}
                  {% if product.type contains "Cotton" %}
                      {%- assign showQuickAdd = true -%}
                  {% else %}
                      {% if product.handle contains 'pack' or product.handle contains 'sets' %}
                      {% else %}
                          {%- assign showQuickAdd = true -%}
                      {% endif %}
                  {% endif %}

                    
                  {% assign prodTitleColor = product.title %}
                  {%- assign colorTitle = prodTitleColor | split: " Only " -%}

                  {% assign premImgFileName = prodColorTitle | replace: " ", "" | append: "---" | append: prdImgFileName | append: "---missy---tile_fr.jpg" %}

                  <div
                    style="position:relative;"
                    data-cypdptype="{{ product.type }}"
                    class="col products_col velaProBlock quickadd epcWrapper item {% if isOnHolidaySale %}this_sale{% endif %}"
                    data-size-guid="{% render 'collection-selectors-charts', product_target: product %}"
                    data-prodtitle="{{ product.title }}"
                    data-prodsku="{{ product.id }}"
                    data-prodtype="{{ product.type }}"
                    data-prodprice="{{ product.price | money_without_currency }}"
                  >
                    <div class="col-wrap velaProBlockInner">
                      {% if hasFlag == "true" %}
                      <div class="prodTileFlag" style="z-index:2;">
                        <div class="prodTileFlagInnerWrapper">
                          <span>{{ prodFlag }}</span>
                        </div>
                      </div>
                      {% endif %}
                      <div class="product-image proHImage d-flex flexJustifyCenter">
                        <a class="proFeaturedImage" href="{{ product.url }}">

                          {%- assign image = product.images[0] -%}

                          <img
                            class="img-responsive lazyload"
                            alt="{{ image.alt }}"
                            title="{{ image.alt }}"
                            width="184"
                            height="230"
                            data-src="{{ image | img_url: 'master' }}">
                        </a>
                      </div>
                      <div class="col-pro-details">
                        {% if product.type contains "Bra Pack" %}
                        <h3 href="{{ product.url }}" draggable="false">
                          <span class="r---product-fabric---22 cy-testClFabric " data-cyclfabric="{{prodNameTitle}}">{{ prodNameTitle| capitalize  }}</span>
                          <span class="r---product-color---22 cy-testClName" data-cyclname="{{prodFabricTitle}}">{{ prodFabricTitle | capitalize  }}</span>
                        </h3>
                        {% elsif product.type contains "Season Set" %}
                        <h3 href="{{ product.url }}" draggable="false">
                          <span class="r---product-fabric---22 cy-testClFabric " data-cyclfabric="{{prodFabricTitle}}">{{ prodFabricTitle| capitalize  }}</span>
                          <span class="r---product-color---22 cy-testClName" data-cyclname="{{isBundleSetFrabric}}">{{ isBundleSetFrabric | capitalize  }}</span>
                        </h3>
                        {% else %}
                        <h3 class="eby---main---products" href="{{ product.url }}" draggable="false">
                          <span class="r---product-fabric---22 cy-testClFabric " data-cyclfabric="{{ prodFabricTitle }}">{{ prodFabricTitle }}</span>
                          <span class="r---product-color---22 cy-testClName" data-cyclname="{{prodNameTitle}}">{{ prodNameTitle | capitalize  }}</span>
                        </h3>
                        {% endif %}
                      </div>
                      <div class="product-details proButton clearfix">
                        <div class="quick_btn pre-btn">
                          <div class="productQuickAdd">
                            <button
                              class="btn btnProduct btnProductQuickAdd"
                              data-href="#velaQuickAdd-{{ product.id }}-1"
